<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:dc="http://purl.org/dc/elements/1.1/"
           xmlns:dcterms="http://purl.org/dc/terms/"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
>
  <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="http://www.w3.org/2001/03/xml.xsd" />
  <xs:import namespace="http://purl.org/dc/elements/1.1/" schemaLocation="dc.xsd" />
  <xs:import namespace="http://purl.org/dc/dcmitype/" schemaLocation="dcmitype.xsd"/>

  <xs:element name="dublincore" type="dublincoreType" />
  <xs:complexType name="dublincoreType">
    <xs:sequence>
      <dc:title />
      <xs:element ref="ns:creator" xmlns:ns="http://purl.org/dc/elements/1.1/"/>
      <xs:element ref="ns:subject" xmlns:ns="http://purl.org/dc/elements/1.1/"/>
      <xs:element ref="ns:description" xmlns:ns="http://purl.org/dc/elements/1.1/"/>
      <xs:element ref="ns:publisher" xmlns:ns="http://purl.org/dc/elements/1.1/"/>
      <xs:element ref="ns:contributor" xmlns:ns="http://purl.org/dc/elements/1.1/"/>
      <xs:element ref="ter:created" xmlns:ter="http://purl.org/dc/terms/"/>
      <xs:element ref="ns:type" xmlns:ns="http://purl.org/dc/elements/1.1/"/>
      <xs:element ref="ter:medium" xmlns:ter="http://purl.org/dc/terms/"/>
      <xs:element ref="ns:identifier" xmlns:ns="http://purl.org/dc/elements/1.1/"/>
      <xs:element ref="ns:source" xmlns:ns="http://purl.org/dc/elements/1.1/"/>
      <xs:element ref="ns:language" xmlns:ns="http://purl.org/dc/elements/1.1/"/>
      <xs:element ref="ns:relation" xmlns:ns="http://purl.org/dc/elements/1.1/"/>
      <xs:element ref="ns:coverage" xmlns:ns="http://purl.org/dc/elements/1.1/"/>
      <xs:element ref="ter:license" xmlns:ter="http://purl.org/dc/terms/"/>
      <xs:element ref="ter:audience" xmlns:ter="http://purl.org/dc/terms/"/>
      <xs:element ref="ter:provenance" xmlns:ter="http://purl.org/dc/terms/"/>
      <xs:element ref="ter:rightsHolder" xmlns:ter="http://purl.org/dc/terms/"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>